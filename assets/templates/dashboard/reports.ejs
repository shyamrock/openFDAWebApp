		<div class="fr">
		<h2><small></small></h2>



      <div class="maincontainer">




          </div>

      <div class=" visible-sm visible-xs">
        <h3>VISUAL GRAPHS</h3>
      </div>

      <form role="form"  ng-submit="submit()">

        <div class="form-groups">
          <div class="form-group  col-lg-offset-1 col-lg-6 col-xs-12">
            <input  class="form-control input-lg" ng-model="searchItem" type="text" placeholder="Any word(s) or phase(s) associated with your recall">
          </div>
          <div class="form-group col-lg-2 col-xs-12">
            <select id="searchtype" class="form-control input-lg" ng-model="selectType" >
              <option value="device">Device Recalls</option>
              <option value="drug">Drug Recalls</option>
              <option value="food" selected="selected">Food Recalls</option>
            </select>

          </div>

        </div>

        <div class="button-container col-lg-2 col-xs-4">
          <button type="submit" class="btn login" style="color:#333!important ;padding: 10px 10px">Search  <i class="fa fa-search" ></i></button>
        </div>


      </form>
      <div ng-show="loading" class="loading">LOADING...
        <i class="fa fa-refresh fa-spin"></i>
      </div>
      <div class="row">
        <div class=" col-lg-11 col-xs-12" >
           <highchart id="chart1" config="chartConfig" class="span10"></highchart>
        </div>
      </div>
      <br>
      <div class="span12">

        <div class=" col-lg-11 col-xs-12" >
          <div class=" col-lg-3 col-md-3 col-sm-3 col-xs-12" >
            <a ng-click="swapChartTypeBar()"  class="btn  btn-default ">
              Bar View
            </a>
            <!--<a ng-click="swapChartTypeBar()">Bar</a>-->
          </div>
          <div class=" col-lg-3 col-md-3 col-sm-3 col-xs-12" >
            <a ng-click="swapChartTypeLine()" class="btn  btn-default ">
               Line View
            </a>
            <!--<a ng-click="swapChartTypeLine()">Line <i class="fa fa-line-chart"></i></a>-->
          </div>
          <div class=" col-lg-3 col-md-3 col-sm-3 col-xs-12" >
            <a ng-click="swapChartTypeSpline()" class="btn btn-default"> Spline View</a>
          </div>
        </div>
      </div>







        </div>


      </div>
      </div>
	</snap-content>
