<div>
	<snap-drawer>
		<div class="snap-menu">
			<div class="sidebar-header text-center">
				<img src="images/flat-avatar.png" alt="" class="sidebar-img img-circle">
			</div>
			<ul class="snap-ul">
				<li ng-class="{active: $state.includes('overview')}"><a ui-sref="overview">HOME</a></li>
				<li ng-class="{active: $state.includes('reports')}"><a ui-sref="reports">REPORTS</a></li>
        <li ng-class="{active: $state.includes('trends')}"><a ui-sref="trends">SEARCH TRENDS</a></li>

      </ul>
		</div>
	</snap-drawer>
	<snap-dragger></snap-dragger>
	<snap-content snap-opt-tap-to-close="false" class="bg-dark">
		<a href="" snap-toggle="left" ng-click="menuOpen = !menuOpen">
			<span class="menutoggle">
				<i ng-hide="menuOpen" class="fa fa-bars fa-2x" ></i>
				<i ng-show="menuOpen" class="fa fa-times fa-2x" ></i>
			</span>
		</a>

		<div class="fr">

      <div   class="col-lg-12 col-xs-12" style="padding:0px 0px !important" >

        <table st-table="rowCollection" class="table table-striped">
          <thead>
          <tr>
            <th>Search Date</th>
            <th>User Ip</th>
            <th>Search Type</th>
            <th>Search Term</th>
           </tr>
          </thead>
          <tbody>
          <tr ng-repeat="item in searchTrendsResult.data">
            <td>{{item.searchDate}}</td>
            <td>{{item.ip}}</td>
            <td>{{item.searchType}}</td>
            <td>{{data.searchTerm}}</td>
           </tr>
          </tbody>

          <tfoot>


          <tr>
            <td colspan="5" class="text-center">
              <div st-pagination="" st-items-by-page="itemsByPage" st-displayed-pages="7"></div>
            </td>
          </tr>
          </tfoot>
        </table>

        <!--
        <table class="table" ng-table="tableParams">

          <tr ng-repeat="item in searchTrendsResult.data">
            <td data-title="'User IP'">{{item.searchDate|date : format : timezone}}</td>
            <td data-title="'User IP'">{{item.ip}}</td>
            <td data-title="'Search Type'">{{ item.searchType | uppercase }}</td>
            <td data-title="'Search Term'">{{item.searchTerm|uppercase}}</td>

          </tr>
        </table>
        -->

    </div>



    </div>

	</snap-content>
</div>
